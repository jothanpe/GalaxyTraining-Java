package pe.edu.galaxy.training.java.ejemplo;

public class App01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a;
		int b;
		int c, d;
		
		a = 10;
		b= 5;
		c = a + b;
		d = c * -1;
		
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
	}

}
